!!! 5
html(lang="en")
  head
    title Brawl IO
    link(rel="shortcut icon", href="favicon.ico")
    script(type="text/javascript", src="/socket.io/socket.io.js")
    script(type="text/javascript", src="vendor/ace/src/ace.js")
    script(type="text/javascript", src="vendor/ace/src/mode-javascript.js")
    script(type="text/javascript", src="vendor/ace/src/theme-idle_fingers.js")
    script(type="text/javascript", src="vendor/raphael-min.js")
    != include(bio_inc.dashboard_css)
    != include(bio_inc.dashboard)
    script
      $(function() {
        BrawlIO.initialize("#{session_key}", $(".content"));
      });
  body
    .body
      .crown(style="display: none; text-align:center")
        //
          img(src='client/images/crown.png', height='90px')
      h1(style="margin-top: 5px") Brawl.IO
      .tabs
        .tab.selected#home
          a(href="/") Home
          .subtab#edit
            a(href="#edit_tab") Edit
          .subtab#test
            a(href="#challenge_tab") Challenge
        .tab#reference
          a(href="api") Reference
      .content
        #brawls_tab
          a.logout(href="logout") Log out
        #edit_tab
          #editor
            #toolbar
              a.save(href="javascript:void(0)") Save
            #code
              #ace_editor
        #challenge_tab(style="margin-top: 20px")
          a#king_challenge(href="javascript:void(0)", style="margin-top: 10px; font-size: 32px;") Submit
          .replay
